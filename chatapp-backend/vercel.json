{
  "version": 2,
  "builds": [
    {
      "src": "chatappbackend/asgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12.4",
        "buildCommand": "daphne -b 0.0.0.0 -p 8000 chatappbackend.asgi:application"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "chatappbackend/asgi.py" }]
}
